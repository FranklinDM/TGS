<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <!-- General -->
        <em:id>tgsidebar@franklindm</em:id>
        <em:version>dev-build</em:version>
        <em:type>2</em:type>
        <em:icon64URL>chrome://aios/skin/icon64.png</em:icon64URL>
        <em:homepageURL>https://github.com/FranklinDM/TGS/wiki/</em:homepageURL>
        <em:aboutURL>chrome://aios/content/about.xul</em:aboutURL>
        <em:optionsURL>chrome://aios/content/prefs/prefs.xul</em:optionsURL>

        <!-- Front-end metadata -->
        <em:name>The Good 'ol Sidebar</em:name>
        <em:description>Sidebar control with award-winning user experience!</em:description>

        <!-- Authors -->
        <em:creator>FranklinDM</em:creator>
        <em:contributor>Ingo Wennemaring (creator of AiOS)</em:contributor>

        <!-- Targeted applications -->
        <em:targetApplication>
            <!-- Pale Moon -->
            <Description>
                <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
                <em:minVersion>28.0.0a1</em:minVersion>
                <em:maxVersion>29.*</em:maxVersion>
            </Description>
        </em:targetApplication>
        <em:targetApplication>
            <!-- Applications using Firefox GUID -->
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <!--
                    Pale Moon 30 and above will stop using its own GUID to
                    "further improve extension compatibility"
                 -->
                <em:minVersion>30.0.0a1</em:minVersion>
                <em:maxVersion>30.*</em:maxVersion>
                <!-- Formerly used to detect Basilisk in the Add-ons Site -->
                <em:basilisk>true</em:basilisk>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>
